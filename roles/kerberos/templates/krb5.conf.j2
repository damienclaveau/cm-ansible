[logging]
   default = FILE:/var/log/krb5libs.log
   kdc = FILE:/var/log/krb5kdc.log
   admin_server = FILE:/var/log/kadmind.log

[libdefaults]
   default_realm = {{iam_kdc_realm}}
   dns_lookup_realm = false
   dns_lookup_kdc = false
   ticket_lifetime = 24h
   renew_lifetime = 7d
   forwardable = true

[realms]
   {{iam_kdc_realm}} = {
     kdc = {{iam_kdc_host}}
     admin_server = {{iam_kdc_host}}
   }

[domain_realm]
   {{dns_domain}} = {{iam_kdc_realm}}
   .{{dns_domain}} = {{iam_kdc_realm}}